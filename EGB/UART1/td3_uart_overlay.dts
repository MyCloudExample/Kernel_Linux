/dts-v1/;
/plugin/;

/ {
    compatible = "brcm,bcm2835";
    
    fragment@0 {
        target = <&uart0>;
        __overlay__ {
            status = "okay";
            
            td3_uart {
                compatible = "brightlight,td3_uart";
                status = "okay";
            };
        };
    };
};